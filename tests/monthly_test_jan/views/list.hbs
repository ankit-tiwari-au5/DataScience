<h3><a class = "btn btn-secondary" href="/views"><i class="fa fa-plus"></i>Create New</a>Student List</h3>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Full Name</th>
            <th>User Name</th>
            <th>Email</th>
            <th>City</th>
            <th>MobileNo</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.fullName}}</td>
            <td>{{this.userName}}</td>
            <td>{{this.mobile}}</td>
            <td>{{this.city}}</td>
            <td>{{this.email}}</td>
            <td>
                <a href="/views/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true></i></a>
                <a href="/views/delete/{{this._id}}"onclick= "return confirm('Are you sure you want to delete this record ?')"><i class="fa fa-pencil fa-lg" aria-hidden="true></i></a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>